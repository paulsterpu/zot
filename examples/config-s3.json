{
    "version": "0.1.0-dev",
    "storage": {
        "rootDirectory": "/zot",
        "objectStoreParams": {
            "name": "s3",
            "region": "us-east-2",
            "bucket": "zot-storage",
            "secure": true,
            "skipverify": false
        },
        "subPaths": {
            "/devops": {
                "rootDirectory": "/zot-devops"
            },
            "/atomix": {
                "rootDirectory": "/zot-atomix"
            },
            "/c3": {
                "rootDirectory": "/zot-c3"
            }
        }
    },
    "http": {
        "address": "127.0.0.1",
        "port": "8080",
        "ReadOnly": false
    },
    "log": {
        "level": "debug"
    }
}
